<h1>Listing page_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @page_categories.each do |page_category| %>
  <tr>
    <td><%= page_category.name %></td>
    <td><%= link_to 'Show', page_category %></td>
    <td><%= link_to 'Edit', edit_page_category_path(page_category) %></td>
    <td><%= link_to 'Destroy', page_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Page category', new_page_category_path %>

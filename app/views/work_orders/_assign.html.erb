 <%= form_for @work_order do |f| %>
    <%= f.collection_select(:assigned_to_username, User.where("workorder_assignability = ?",true),:name, :name, :include_blank => true, :class => "simple_form select.required")%>
    <%= f.submit t "global.change" %>
    <%= hidden_field_tag(:id, @work_order.id ) %>
<% end %>
<table width="100%" >
  <tr>
    <th><%= "WO #" %></th>
    <th><%= "Client" %></th>
    <th><%= "Subject" %></th>
    <th><%= "Description" %></th>
    <th><%= "Created at" %></th>
    <th><%= "Created by" %></th>
    <th><%= "Closed by" %></th>
    <th><%= "Closed on" %></th>
  </tr>


<% @closed_work_orders.each do |closed_wo| %>
  <tr align="left"
      <% if closed_wo.priority_list_id == 1 %>
          class="priority_low"
        <% else %>
    <% if closed_wo.priority_list_id == 2 %>
          class="priority_normal"
       <% else %>
      <% if closed_wo.priority_list_id == 3 %>
          class="priority_high"
      <% else %>
  <% if closed_wo.priority_list_id == 4 %>
          class="priority_urgent"
      <% else %>
 <% if closed_wo.priority_list_id == 5 %>
          class="priority_critical"
      <% else %>
  <% if closed_wo.priority_list_id > 5 %>
      class=""
      <% end %>
      <% end %>
      <% end %>
      <% end %>
      <% end %>
      <% end %>
       >
    <td><%= link_to(closed_wo.id, closed_wo) %></td>
    <td><%= link_to(closed_wo.user.username, user_path(closed_wo.user_id)) %></td>
    <td><%=h closed_wo.subject %></td>
    <td><%=h closed_wo.description %></td>
    <td><%=h l closed_wo.created_at, :format => :medium %></td>
    <td><%=h closed_wo.created_by %></td>
    <td><%=h closed_wo.closed_by %></td>
    <td><%= time_ago_in_words(closed_wo.closed_date) %> <%= t "global.ago" %> </td>
  </tr>
<% end %>
</table>